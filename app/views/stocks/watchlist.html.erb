<h1>My Watchlist</h1>
  <div class="table-responsive">
    <table class="table">
      <thead style="font-weight:bold">
        <tr>
          <td>Symbol</td>
          <td>Value</td>
          <td>50 day Moving Avg</td>
          <td>52 week high</td>
          <td>Remove from Watchlist</td>
        </tr>
      </thead>
      <tbody>
      <% @data.each do |stock| %>
        <tr>
          <td><%= stock.symbol %> </td>
          <td>$<%= stock.last_trade_price %></td>
          <td>$<%= stock.moving_average_50_day %></td>
          <td>$<%= stock.high_52_weeks %></td>
          <td><form action="/delete" method="delete">
                <input type="hidden" name="stocktodelete" value=<%=stock.symbol%>>
                <button type="submit" class="btn btn-danger" >Remove</button>
             </form></td>
        </tr>
      <%end%>
      <tbody>
    </table>
  </div>