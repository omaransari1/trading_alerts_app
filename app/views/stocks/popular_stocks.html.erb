<h1>Top Stocks to Watch</h1>
  <div class="table-responsive">
    <table class="table">
      <thead style="font-weight:bold">
        <tr>
          <td>Symbol</td>
          <td>Watchers</td>
        </tr>
      </thead>
      <tbody>
      <% @counts.each do |k, v| %>
        <tr>
          <td><%=link_to "#{k}", "/stocks/stockprofile/#{k}" %> </td>
          <td><%= v %></td>
        </tr>
      <%end%>
      <tbody>
    </table>
    <br>
    <%= link_to 'Home', root_path %>
  </div>